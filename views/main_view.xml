<odoo>
    <record id="view_custom_model_init" model="ir.ui.view">
        <field name="name">view.custom.model.init</field>
        <field name="model">custom.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="view_custom_model_form" model="ir.ui.view">
        <field name="name">view.custom.model.init</field>
        <field name="model">custom.model</field>
        <field name="arch" type="xml">
            <form>
                <field name="name"/>
            </form>
        </field>
    </record>

